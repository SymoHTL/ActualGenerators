{
  "multipart": [
    {
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_base"
      }
    },


    {
      "when": {
        "AND": [
          {"north" : "true"},
          {"north_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_none"
      }
    },{
      "when": {
        "AND": [
          {"north" : "true"},
          {"north_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_insert"
      }
    },{
      "when": {
        "AND": [
          {"north" : "true"},
          {"north_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_extract"
      }
    },{
      "when": {
        "AND": [
          {"north" : "true"},
          {"north_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_both"
      }
    },{
      "when": {
        "AND": [
          {"north" : "true"},
          {"north_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_none"
      }
    },


    {
      "when": {
        "AND": [
          {"east" : "true"},
          {"east_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_none",
        "x" : 90
      }
    },{
      "when": {
        "AND": [
          {"east" : "true"},
          {"east_type" : "input" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_insert",
        "x" : 90
      }
    },{
      "when": {
        "AND": [
          {"east" : "true"},
          {"east_type" : "output" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_extract",
        "x" : 90
      }
    },{
      "when": {
        "AND": [
          {"east" : "true"},
          {"east_type" : "both" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_both",
        "x" : 90
      }
    },


    {
      "when": {
        "AND": [
          {"south" : "true"},
          {"south_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_none",
        "x" : 180
      }
    },{
      "when": {
        "AND": [
          {"south" : "true"},
          {"south_type" : "input" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_insert",
        "x" : 180
      }
    },{
      "when": {
        "AND": [
          {"south" : "true"},
          {"south_type" : "output" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_extract",
        "x" : 180
      }
    },{
      "when": {
        "AND": [
          {"south" : "true"},
          {"south_type" : "both" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_both",
        "x" : 180
      }
    },


    {
      "when": {
        "AND": [
          {"west" : "true"},
          {"west_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_none",
        "x" : 270
      }
    },{
      "when": {
        "AND": [
          {"west" : "true"},
          {"west_type" : "input" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_insert",
        "x" : 270
      }
    },{
      "when": {
        "AND": [
          {"west" : "true"},
          {"west_type" : "output" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_extract",
        "x" : 270
      }
    },{
      "when": {
        "AND": [
          {"west" : "true"},
          {"west_type" : "both" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_horizontal_both",
        "x" : 270
      }
    },


    {
      "when": {
        "AND": [
          {"bottom" : "true"},
          {"down_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_none"
      }
    },{
      "when": {
        "AND": [
          {"bottom" : "true"},
          {"down_type" : "input" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_insert"
      }
    },{
      "when": {
        "AND": [
          {"bottom" : "true"},
          {"down_type" : "output" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_extract"
      }
    },{
      "when": {
        "AND": [
          {"bottom" : "true"},
          {"down_type" : "both" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_both"
      }
    },


    {
      "when": {
        "AND": [
          {"top" : "true"},
          {"up_type" : "cable" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_none",
        "y": 180
      }
    },{
      "when": {
        "AND": [
          {"top" : "true"},
          {"up_type" : "input" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_insert",
        "y": 180
      }
    },{
      "when": {
        "AND": [
          {"top" : "true"},
          {"up_type" : "output" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_extract",
        "y": 180
      }
    },{
      "when": {
        "AND": [
          {"top" : "true"},
          {"up_type" : "both" }
        ]
      },
      "apply": {
        "model": "actualgenerators:block/pipe/pipe_vertical_both",
        "y": 180
      }
    }
  ]
}
